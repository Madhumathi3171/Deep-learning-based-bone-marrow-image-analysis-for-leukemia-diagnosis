def DC_CBAM_InceptionV3(input_shape=(128,128,3), num_classes=5):
    inputs = Input(shape=input_shape)

    # Pretrained InceptionV3 backbone
    base_model = InceptionV3(include_top=False, input_tensor=inputs, weights='imagenet')
    base_model.trainable = False  # Freeze backbone initially

    x = base_model.output  # Feature maps from backbone

    # Dual-Channel Attention
    x1 = layers.Conv2D(64, 3, padding='same', activation='relu')(x)
    x1 = CBAMBlock()(x1)  # Use custom layer

    x2 = layers.Conv2D(64, 5, padding='same', activation='relu')(x)
    x2 = CBAMBlock()(x2)  # Use custom layer

    # Fuse channels
    x = layers.Concatenate()([x1, x2])
    x = layers.Conv2D(128, 3, padding='same', activation='relu')(x)
    x = layers.GlobalAveragePooling2D()(x)
    x = layers.Dropout(0.5)(x)

    outputs = layers.Dense(num_classes, activation='softmax')(x)

    model = Model(inputs, outputs)
    return model
